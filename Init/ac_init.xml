<?xml version="1.0" encoding="UTF-8"?>

<!-- Koliber init dialog based on acconfig from 737 Max. -->

<PropertyList>

    <name>init</name>
    <layout>vbox</layout>
    <pref-width>260</pref-width>

    <group>
        <layout>hbox</layout>
        <text>
            <halign>left</halign>
            <label>Please wait...</label>
        </text>
    </group>

    <hrule />

    <group>
        <layout>vbox</layout>

        <text>
            <halign>left</halign>
            <label>Loading</label>
        </text>
        <text>
            <border>1</border>
            <label>9</label>
            <valign>center</valign>
            <property>init/state</property>
            <live>1</live>
        </text>

    </group>

</PropertyList>