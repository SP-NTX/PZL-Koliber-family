<?xml version="1.0"?>
<PropertyList>

  <path>safe-flight.ac</path>

  <animation>
    <type>textranslate</type>
    <object-name>light_bulb_safe_flight</object-name>
    <property>instrumentation/stall-warn/lamp-lit</property>
    <bias>0.0001</bias>
    <factor>0.85</factor>
    <step>1.0</step>
    <axis>
      <x>0</x>
      <y>-1</y>
    </axis>
  </animation>

  <animation>
    <type>material</type>
    <object-name>light_bulb_safe_flight</object-name>

    <ambient>
      <red>1.0</red>
      <green>0.0</green>
      <blue>0.0</blue>
    </ambient>
    <diffuse>
      <red>1.0</red>
      <green>0.0</green>
      <blue>0.0</blue>
    </diffuse>
    <specular>
      <red>1.0</red>
      <green>0.5</green>
      <blue>0.5</blue>
    </specular>
    <emission>
      <red>1.0</red>
      <green>0.0</green>
      <blue>0.0</blue>
      <factor-prop>instrumentation/stall-warn/lamp-lit</factor-prop>
    </emission>
    <transparency>
      <alpha>0.90</alpha>
    </transparency>
  </animation>
  <animation>
    <type>material</type>
    <object-name>bulb_safe_flight</object-name>
    <transparency>
      <alpha>0.4</alpha>
    </transparency>
    <emission>
      <red>1.5</red>
      <green>0.5</green>
      <blue>0.5</blue>
      <factor-prop>instrumentation/stall-warn/lamp-lit</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>light_emmiter_safe_flight</object-name>
    <emission>
      <red>21.5</red>
      <green>0.5</green>
      <blue>0.5</blue>
      <factor-prop>instrumentation/stall-warn/lamp-lit</factor-prop>
    </emission>
    <transparency>
      <alpha>0.4</alpha>
    </transparency>
  </animation>
  <animation>
    <type>noshadow</type>
    <object-name>light_bulb_safe_flight</object-name>
  </animation>


